{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "browser": true,
    "jquery": true
  },
  "parserOptions": {
        "ecmaVersion": 6,
         "sourceType": "module"
  },
  "globals": {
      "d3": false, /* false means we can't overwrite it */
      "Set": false
  },
  "rules": {
    require-jsdoc: "off",
    no-console: "off",
    quotes: ["error", "double"],
    max-len: ["error", { "ignoreTrailingComments": true,
                         "ignoreStrings": true }]
  }
}
